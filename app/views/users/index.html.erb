<% provide(:title, 'All users') %>
<h1>All users</h1>
<%= paginate @users, theme: "twitter-bootstrap-3"  %>
<ul class="users">
  <% @users.each do |user| %>
    <li>
      <%= gravatar_for user, size: 50 %>
      <%= link_to user.name, user %>
    </li>
  <% end %>
</ul>
<%= paginate @users, theme: "twitter-bootstrap-3"  %>
